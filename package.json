{
  "name": "cavuca-data",
  "version": "0.0.1",
  "description": "api to serve brasilian datasets",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andre-novais/CavucaData.git"
  },
  "keywords": [
    "data",
    "brazil",
    "webscraping",
    "datamining"
  ],
  "author": "Andr√© Novais",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andre-novais/CavucaData/issues"
  },
  "homepage": "https://github.com/andre-novais/CavucaData#readme",
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/platform-express": "^7.0.0",
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/mongodb": "^3.5.27",
    "@wdio/cli": "6.4.7",
    "@wdio/local-runner": "6.4.7",
    "@wdio/mocha-framework": "6.4.7",
    "@wdio/spec-reporter": "6.4.7",
    "@wdio/sync": "6.4.7",
    "chai": "4.2.0",
    "chromedriver": "85.0.1",
    "express": "4.17.1",
    "fs": "0.0.1-security",
    "method-override": "3.0.0",
    "mocha": "8.1.3",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "wdio-chromedriver-service": "6.0.4",
    "wdio-json-reporter": "2.0.0",
    "webdriverio": "6.4.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.3",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "lodash": "^4.17.20",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "preset": "@shelf/jest-mongodb",
    "verbose": false
  }
}
